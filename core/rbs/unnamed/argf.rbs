      #     ARGF.readbyte  #=> 10
      #     ARGF.readbyte  #=> end of file reached (EOFError)
      #
      %a{annotate:rdoc:copy:ARGF#readbyte}
      def readbyte: () -> Integer

      # <!--
      #   rdoc-file=io.c
      #   - ARGF.readchar  -> String or nil
      # -->
      # Reads the next character from ARGF and returns it as a String. Raises an
