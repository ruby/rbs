      # <!--
      #   rdoc-file=io.c
      #   - ARGF.set_encoding(ext_enc)                -> ARGF
      #   - ARGF.set_encoding("ext_enc:int_enc")      -> ARGF
      #   - ARGF.set_encoding(ext_enc, int_enc)       -> ARGF
